{
  "name": "nogizaka-lib-redesign",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "stylelint": "stylelint '**/*.scss'  --fix",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "framer-motion": "^1.6.6",
    "gatsby": "^2.13.3",
    "gatsby-plugin-layout": "^1.1.3",
    "gatsby-plugin-manifest": "^2.2.12",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.1.12",
    "gatsby-plugin-typescript": "^2.1.5",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-json": "^2.2.2",
    "node-sass": "^4.12.0",
    "query-string": "^6.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-intl": "^3.1.7",
    "react-lazyload": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^12.0.12",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.9",
    "@types/react-lazyload": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "gatsby-plugin-root-import": "^2.0.5",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.10.0",
    "typescript": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
